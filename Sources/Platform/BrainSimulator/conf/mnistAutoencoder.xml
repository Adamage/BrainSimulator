<?xml version="1.0" encoding="utf-8"?>
<network
  inputwidth="28" inputheight="28" inputcount="1" samplesperstep="1"
	outputMinValue="0" outputMaxValue="1" outputColumnHint="28"
	>
  <convolutionLayer 	id="conv1" 	nbFeatures="8" 	patchWidth="5" patchHeight="5" />
  <activationLayer 	function="tanh" />
  <poolLayer 			id ="pool1" stride="2" 		rule="AVERAGE" />
  <convolutionLayer 	id="conv2" 	nbFeatures="16" patchWidth="5" patchHeight="5" />
  <activationLayer 	function="tanh" />
  <poolLayer 			id ="pool2" stride="2" 		rule="AVERAGE" />
  <neuronLayer		id="neur1" 	nbNeurons="10"/>

  <featureLayer/>
  
  <mirrorNeuronLayer 		original="neur1" />
  <mirrorPoolLayer 		original="pool2" />
  <mirrorConvolutionLayer original="conv2" />
  <activationLayer 	function="tanh" />
  <mirrorPoolLayer 		original="pool1" />
  <mirrorConvolutionLayer original="conv1" />
  <activationLayer 	function="tanh" />
</network>
